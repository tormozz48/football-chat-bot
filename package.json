{
  "name": "football-chat-bot",
  "version": "1.0.0",
  "description": "Telegram bot for football weekend chat",
  "main": "src/index.js",
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "node_modules/.bin/babel src -d dist",
    "postinstall": "npm run build",
    "lint": "eslint .",
    "test-unit": "mocha test",
    "test": "npm run lint && npm run test-unit",
    "cover": "istanbul cover _mocha test",
    "coveralls": "npm i coveralls && npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "precommit": "npm run lint",
    "start-dev": "nodemon src/index.js",
    "start-prod": "NODE_ENV=production node dist/index.js",
    "start": "npm run start-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tormozz48/football-chat-bot.git"
  },
  "keywords": [
    "football",
    "telegram",
    "chat",
    "boot",
    "weekend",
    "api",
    "funny"
  ],
  "author": "andrey.kuznetsov48@yandex.ru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tormozz48/football-chat-bot/issues"
  },
  "homepage": "https://github.com/tormozz48/football-chat-bot#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "config": "^1.28.1",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "got": "^8.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "mongoose": "^4.13.6",
    "node-cron": "^1.2.1",
    "telegraf": "^3.17.1",
    "telegraf-command-parts": "^1.0.3",
    "telegraf-logger": "^2.0.2",
    "telegraf-wit": "^2.1.0",
    "yahoo-weather": "^2.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.12.1",
    "eslint-config-gemini-testing": "^2.7.0",
    "husky": "^0.14.3",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.5",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0"
  }
}
